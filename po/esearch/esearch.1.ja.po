#: en/esearch/esearch.1:66
msgid "Homepage"
msgstr "ホームページ"

#. type: TP
#: en/esearch/esearch.1:66
#, no-wrap
msgid "B<%d>"
msgstr "B<%d>"

#. type: Plain text
#: en/esearch/esearch.1:69
msgid "Description"
msgstr "説明"

#. type: SH
#: en/esearch/esearch.1:70
#, no-wrap
msgid "EXAMPLES"
msgstr "例"

#. type: TP
#: en/esearch/esearch.1:71
#, no-wrap
msgid "CW<esearch -o \"/usr/portage/%p/ChangeLog\\en\" ^package$>"
msgstr "CW<esearch -o \"/usr/portage/%p/ChangeLog\\en\" ^package$>"

#. type: Plain text
#: en/esearch/esearch.1:74
msgid "Print the correct path to the ChangeLog file of 'package'."
msgstr "'package' の ChangeLog への正しいパスを表示する。"

#. type: TP
#: en/esearch/esearch.1:74
#, no-wrap
msgid "CW<esearch -o \"%n-%va.ebuild\\en\" pattern>"
msgstr "CW<esearch -o \"%n-%va.ebuild\\en\" pattern>"

#. type: Plain text
#: en/esearch/esearch.1:77
msgid "Print the names of the current ebuild files."
msgstr "現在の ebuild ファイルの名前を表示する。"

#. type: TP
#: en/esearch/esearch.1:77
#, no-wrap
msgid "CW<esearch -o \"%p\\en\" ^ E<gt> package-list>"
msgstr "CW<esearch -o \"%p\\en\" ^ E<gt> package-list>"

#. type: Plain text
#: en/esearch/esearch.1:80
msgid "Generate a list of all available packages."
msgstr "利用可能な全パッケージ一覧を生成する。"

