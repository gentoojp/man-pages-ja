#: en/esearch/esearch.1:60
#, no-wrap
msgid "B<%s>"
msgstr "B<%s>"

#. type: Plain text
#: en/esearch/esearch.1:63
msgid "size of downloaded files"
msgstr "ダウンロードされたファイルのサイズ"

#. type: TP
#: en/esearch/esearch.1:63
#, no-wrap
msgid "B<%h>"
msgstr "B<%h>"

