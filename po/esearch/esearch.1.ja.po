#: en/esearch/esearch.1:54
#, no-wrap
msgid "B<%va>"
msgstr "B<%va>"

#. type: Plain text
#: en/esearch/esearch.1:57
msgid "latest version available"
msgstr "利用可能な最新バージョン"

#. type: TP
#: en/esearch/esearch.1:57
#, no-wrap
msgid "B<%vi>"
msgstr "B<%vi>"

#. type: Plain text
#: en/esearch/esearch.1:60
msgid "latest version installed, or \"\""
msgstr ""
"インストールされている最新バージョン、インストールされていなければ \"\""

