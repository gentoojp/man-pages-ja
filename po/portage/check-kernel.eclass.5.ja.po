#, no-wrap
msgid "VARIABLES"
msgstr "変数"

#. type: Plain text
#: en/portage/check-kernel.eclass.5:9
msgid "The following variables are exported by the B<check-kernel> eclass:"
msgstr "次の変数が B<check-kernel> eclass によって用意されている:"

#. type: TP
#: en/portage/check-kernel.eclass.5:9
#, no-wrap
msgid "B<KV_full>"
msgstr "B<KV_full>"

#. type: Plain text
#: en/portage/check-kernel.eclass.5:12
msgid "Full kernel version (eg. 2.4.27-gentoo-r1)"
msgstr "完全なカーネルバージョン (例: 2.4.27-gentoo-r1)"

#. type: TP
#: en/portage/check-kernel.eclass.5:12
#, no-wrap
msgid "B<KV_major>"
msgstr "B<KV_major>"

#. type: Plain text
#: en/portage/check-kernel.eclass.5:15
msgid "Major kernel version (eg. 2)"
msgstr "カーネルバージョンのメジャー番号 (例: 2)"

#. type: TP
#: en/portage/check-kernel.eclass.5:15
#, no-wrap
msgid "B<KV_minor>"
msgstr "B<KV_minor>"

#. type: Plain text
#: en/portage/check-kernel.eclass.5:18
msgid "Minor kernel version (eg. 4)"
msgstr "カーネルバージョンのマイナー番号 (例: 4)"

#. type: TP
#: en/portage/check-kernel.eclass.5:18
#, no-wrap
msgid "B<KV_micro>"
msgstr "B<KV_micro>"

#. type: Plain text
#: en/portage/check-kernel.eclass.5:21
msgid "Micro kernel version (eg. 27)"
msgstr "カーネルバージョンのマイクロ番号 (例: 27)"

#. type: SH
#: en/portage/check-kernel.eclass.5:21 en/portage/cvs.eclass.5:59
